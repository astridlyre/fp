{
  "compilerOptions": {
    "target": "es2021",
    "allowJs": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "module": "esnext",
    "moduleResolution": "node"
  }
}
